{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "default": [],
  "title": "Root Schema",
  "items": {
    "type": "object",
    "default": {},
    "title": "A Schema",
    "required": [
      "type",
      "setup",
      "punchline",
      "id"
    ],
    "properties": {
      "type": {
        "type": "string",
        "minLength": 1,
        "default": "",
        "title": "The type Schema",
        "examples": [
          "knock-knock"
        ]
      },
      "setup": {
        "type": "string",
        "minLength": 1,
        "default": "",
        "title": "The setup Schema",
        "examples": [
          "Knock knock. \n Who's there? \n A broken pencil. \n A broken pencil who?"
        ]
      },
      "punchline": {
        "type": "string",
        "minLength": 1,
        "default": "",
        "title": "The punchline Schema",
        "examples": [
          "Never mind. It's pointless."
        ]
      },
      "id": {
        "type": "integer",
        "default": 0,
        "title": "The id Schema",
        "examples": [
          12
        ]
      }
    },
    "examples": [
      {
        "type": "knock-knock",
        "setup": "Knock knock. \n Who's there? \n A broken pencil. \n A broken pencil who?",
        "punchline": "Never mind. It's pointless.",
        "id": 12
      }
    ]
  },
  "examples": [
    [
      {
        "type": "knock-knock",
        "setup": "Knock knock. \n Who's there? \n A broken pencil. \n A broken pencil who?",
        "punchline": "Never mind. It's pointless.",
        "id": 12
      }
    ]
  ]
}